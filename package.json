{
  "name": "js-fp-koans",
  "version": "1.0.0",
  "description": "A set of programming koans for learning js-fp",
  "main": "index.js",
  "scripts": {
    "lint": "eslint './src/**/*.js'",
    "mocha": "mocha --compilers js:babel-core/register",
    "test:1": "npm run mocha -- 'src/1-*.js' || true",
    "test:2": "npm run mocha -- 'src/2-*.js' || true",
    "test:3": "npm run mocha -- 'src/3-*.js' || true",
    "test:4": "npm run mocha -- 'src/4-*.js' || true",
    "test:5": "npm run mocha -- 'src/5-*.js' || true",
    "test:6": "npm run mocha -- 'src/6-*.js' || true",
    "test:7": "npm run mocha -- 'src/7-*.js' || true",
    "test:8": "npm run mocha -- 'src/8-*.js' || true",
    "test": "(npm run mocha -- 'src/*.js') || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrRacoon/js-fp-koans.git"
  },
  "author": "Erik Sutherland <erik.misu@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MrRacoon/js-fp-koans/issues"
  },
  "homepage": "https://github.com/MrRacoon/js-fp-koans#readme",
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.3.0",
    "eslint-plugin-babel": "^4.1.1",
    "mocha": "^3.4.2",
    "npm-run-all": "^4.0.2",
    "ramda": "^0.24.1",
    "ramda-fantasy": "^0.8.0",
    "sinon": "^2.3.8",
    "sinon-chai": "^2.12.0",
    "eslint-config-google": "^0.9.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "google",
    "env": {
      "node": true
    },
    "plugins": [
      "babel"
    ]
  }
}
